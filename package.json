{
  "name": "@baselime/node-opentelemetry",
  "version": "0.1.0",
  "description": "Instrument node.js applications with open telemetry ",
  "type": "module",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/index.map.js"
  ],
  "main": "dist/index.js",
  "devDependencies": {
    "@types/node": "^20.8.6",
    "esbuild": "^0.19.4",
    "mockttp": "^3.9.4",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "scripts": {
    "build:sdk": "esbuild src/index.ts --bundle --sourcemap --target=node18 --platform=node --outfile=dist/index.js",
    "build:types": "tsc src/index.ts --declaration --sourcemap --outDir dist",
    "build": "npm run build:types & npm run build:sdk",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baselime/node-opentelemetry.git"
  },
  "keywords": [
    "baselime",
    "otel",
    "opentelemetry",
    "tracing",
    "node.js"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baselime/node-opentelemetry/issues"
  },
  "homepage": "https://github.com/baselime/node-opentelemetry#readme",
  "dependencies": {
    "@opentelemetry/exporter-trace-otlp-http": "^0.44.0",
    "@opentelemetry/instrumentation": "^0.44.0",
    "@opentelemetry/instrumentation-http": "^0.44.0",
    "@opentelemetry/resource-detector-aws": "^1.3.2",
    "@opentelemetry/resources": "^1.17.1",
    "@opentelemetry/sdk-node": "^0.44.0",
    "@opentelemetry/sdk-trace-node": "^1.17.1"
  }
}
