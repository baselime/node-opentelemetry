{
  "name": "@baselime/node-opentelemetry",
  "version": "0.4.3",
  "description": "Instrument node.js applications with open telemetry ",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./lambda": {
      "import": "./dist/lambda.js",
      "require": "./dist/lambda.cjs",
      "types": "./dist/lambda.d.ts"
    },
    "./trpc": {
      "import": "./dist/trpc.js",
      "require": "./dist/trpc.cjs",
      "types": "./dist/trpc.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@release-it/keep-a-changelog": "^4.0.0",
    "@types/node": "^20.8.6",
    "esbuild": "^0.19.4",
    "mockttp": "^3.9.4",
    "patch-package": "^8.0.0",
    "release-it": "^16.2.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsup",
    "test": "vitest",
    "release": "release-it",
    "prebuild": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baselime/node-opentelemetry.git"
  },
  "keywords": [
    "baselime",
    "otel",
    "opentelemetry",
    "tracing",
    "node.js"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baselime/node-opentelemetry/issues"
  },
  "homepage": "https://github.com/baselime/node-opentelemetry#readme",
  "peerDependencies": {
    "@trpc/server": "^10.0.0 || ^11.0.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.48.0",
    "@opentelemetry/instrumentation": "^0.48.0",
    "@opentelemetry/instrumentation-http": "^0.48.0",
    "@opentelemetry/resource-detector-aws": "^1.3.3",
    "@opentelemetry/resources": "^1.21.0",
    "@opentelemetry/sdk-node": "^0.48.0",
    "@opentelemetry/sdk-trace-node": "^1.21.0",
    "@types/aws-lambda": "^8.10.134",
    "axios": "^1.6.0",
    "flat": "^6.0.1",
    "shimmer": "^1.2.1",
    "undici": "^5.26.3"
  }
}
